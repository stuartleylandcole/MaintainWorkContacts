using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace MaintainWorkContacts.Utilities
{
    public static class ListUtils
    {
        public static void AddIfNotNull<T>(this List<T> list, T toAdd)
        {
            if (toAdd != null)
            {
                list.Add(toAdd);
            }
        }
    }
}
